{
	"$schema": "http://json-schema.org/draft-03/schema#",
	"name": "oauth-credentials",
	"type": "object",
	"properties": {
		"consumer_secret": {
			"type": "string",
			"required": true
		},
		"consumer_key": {
			"type": "string",
			"required": true
		},
		"token": {
			"type": "string",
			"required": true
		},
		"token_secret": {
			"type": "string",
			"required": true
		}
	}
}
